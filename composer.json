{
    "name": "solvrtech/laravel-logbook",
    "description": "The Package provide integrate the client app into logbook app.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Solvrtech\\Laravel\\Logbook\\": "src/"
        }
    },
    "authors": [
        {
            "name": "solvrtech"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "~8.0.0 || ~8.1.0 || ~8.2.0",
        "monolog/monolog": "~2.0 || ~3.0",
        "psr/log": "^3.0",
        "illuminate/contracts": "~9.0 || ~10.0",
        "illuminate/console": "~9.0 || ~10.0",
        "illuminate/support": "~9.0 || ~10.0",
        "symfony/http-client": "~5.0 || ~6.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Solvrtech\\Laravel\\Logbook\\LogbookServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}